{
  "manifest_version": 2,

  "name": "Quoteboy",
  "description": "This extension will bring us the quote for the requested tag",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],

  "content_scripts": [ 
        { 
            "js": [ "jquery-3.4.1.min.js"],
             "matches": [ "http://*/*", "https://*/*"]
         }
     ],
  
  "key": ""
}